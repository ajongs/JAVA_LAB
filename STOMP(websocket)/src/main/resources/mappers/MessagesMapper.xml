<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.stomp.mapper.MessagesMapper">
    <insert id="uploadMessage" parameterType="Message">
        INSERT INTO messages
        VALUES (#{id}, #{receiver}, #{sender}, #{messageContent}, now())
    </insert>
</mapper>